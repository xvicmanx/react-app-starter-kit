{
  "name": "react-app-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for developing React apps",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:dev-server lint:watch check:security test:watch",
    "lint": "esw --color src/ scripts/",
    "lint:watch": "npm run lint -- --watch",
    "check:security": "nsp check --reporter summary",
    "web-accessibility:audit": "babel-node scripts/web-accessibility-audit.js",
    "start-and-share:development": "npm-run-all --parallel start share:development",
    "start:dev-server": "babel-node scripts/start-server.js",
    "share:development": "babel-node scripts/share-development.js",
    "deploy-app": "babel-node scripts/deploy-app.js",
    "test:watch": "npm run test -- --watchAll",
    "test": "jest",
    "build:assets": "NODE_ENV=production babel-node scripts/build.js",
    "generate:static-html": "react-snapshot",
    "generate:critical": "babel-node scripts/generate-critical.js",
    "clean": "rimraf ./build && mkdir build",
    "prebuild": "npm-run-all clean test lint",
    "build": "npm-run-all build:assets generate:static-html generate:critical",
    "postbuild": "babel-node scripts/serve-build.js"
  },
  "keywords": [
    "React",
    "Starter",
    "kit"
  ],
  "author": "Victor Trejo",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "critical": "^1.2.2",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.4",
    "express": "^4.16.3",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "handlebars-loader": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "localtunnel": "^1.9.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "open": "0.0.5",
    "react-test-renderer": "^16.3.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "scp2": "^0.5.0",
    "style-loader": "^0.20.3",
    "surge": "^0.20.1",
    "wcag": "^0.3.0",
    "webpack": "^4.5.0",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-snapshot": "^1.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/assetsTransformer.js"
    }
  }
}
