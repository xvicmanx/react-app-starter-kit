{
  "name": "react-app-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for developing React apps",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:dev-server lint:watch test:watch",
    "lint": "esw --color src/ scripts/",
    "lint:watch": "npm run lint -- --watch",
    "check:security": "nsp check --reporter summary",
    "web-accessibility:audit": "babel-node scripts/web-accessibility-audit.js",
    "start-and-share:development": "npm-run-all --parallel start share:development",
    "start:dev-server": "webpack-dev-server --config ./config/webpack.config.js --open",
    "share:development": "babel-node scripts/share-development.js",
    "share:build": "babel-node scripts/share-build.js",
    "deploy-app": "babel-node scripts/deploy-app.js",
    "test:watch": "npm run test -- --watchAll",
    "test": "jest ./src/__unit-tests__",
    "test:e2e": "jest ./src/__e2e-tests__",
    "build:assets": "NODE_ENV=production babel-node scripts/build.js",
    "generate:static-html": "react-snapshot",
    "generate:critical": "babel-node scripts/generate-critical.js",
    "clean": "rimraf ./build && mkdir build",
    "prebuild": "npm-run-all clean test lint",
    "build": "npm-run-all build:assets generate:static-html generate:critical",
    "postbuild": "babel-node scripts/minify-service-worker.js",
    "serve:build": "NODE_ENV=production babel-node scripts/serve-build.js",
    "pwa-report": "mkdirp .tmp && lighthouse --view --output html --output-path ./.tmp/report.html",
    "pwa-report:build": "npm run pwa-report http://localhost:3001"
  },
  "keywords": [
    "React",
    "Starter",
    "kit"
  ],
  "author": "Victor Trejo",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "copy-webpack-plugin": "^4.5.1",
    "critical": "^1.2.2",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.4",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "handlebars-loader": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "ip": "^1.1.5",
    "jest": "^22.4.3",
    "lighthouse": "^2.9.4",
    "localtunnel": "^1.9.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "open": "0.0.5",
    "puppeteer": "^1.3.0",
    "react-test-renderer": "^16.3.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "scp2": "^0.5.0",
    "style-loader": "^0.20.3",
    "surge": "^0.20.1",
    "uglify-js": "^3.3.22",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "wcag": "^0.3.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "offline-plugin": "^4.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-snapshot": "^1.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/assetsTransformer.js"
    }
  }
}
